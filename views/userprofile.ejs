<%-include("partials/header")-%>
<main role="main">
    <div class="main-header ">
        <h5 class="form-title pt-3"> معلومات المستخدم </h5>
        <h3 class="pb-3 " dir="ltr">+213-555-811-350</h3>
    </div>
    <form class="form-sign mt-3">

        <div class="mb-2 align-right">
            <div class="form-check form-check-inline  ">
                <h5 class="mr-2">اسجل كــ</h5>
                <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio1" value="option1"
                    onclick="handleClick(this);">
                <label class="form-check-label" for="inlineRadio1">&nbsp;متبرع</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="radio" name="inlineRadioOptions" id="inlineRadio2" value="option2"
                    onclick="handleClick(this);">
                <label class="form-check-label" for="inlineRadio2">&nbsp;مريض</label>
            </div>
        </div>
        <div class="align-right">
            <h5 class="mb-2 "> حدد زمرة دمك</h5>
        </div>
        <div class="btn-group mb-2 cbt" data-toggle="buttons">

            <label class="btn btn-danger  btn-bt active">A+</label>
            <label class="btn btn-danger  btn-bt"> A- </label>
            <label class="btn btn-danger  btn-bt"> B+ </label>
            <label class="btn btn-danger  btn-bt"> B- </label>
            <label class="btn btn-danger  btn-bt"> O+ </label>
            <label class="btn btn-danger  btn-bt"> O- </label>
            <label class="btn btn-danger  btn-bt"> AB+ </label>
            <label class="btn btn-danger  btn-bt"> AB- </label>
        </div>
        <input type="tel" id="inputCodePost" class="form-control mb-2" placeholder="رقم الولاية" required autocomplete="off"
            autofocus onfocus="this.value=''">
        <input type="captcha" id="captcha" class="form-control mb-4" placeholder="5+9 =?" required autocomplete="off"
            autofocus onfocus="this.value=''">

        <!--           <div class=" checkbox mb-3">
          <label>
            <input type="checkbox" value="remember-me"> Remember me
          </label>
        </div> -->
        <button class="btn btn-md btn-default btn-block mb-3" type="submit">تعديل</button>

    </form>
</main>
<%-include("partials/footer")-%>